deploymentMode: "SelectorSyncSet"
selectorSyncSet:
  matchLabels:
    api.openshift.com/managed: "true"
    api.openshift.com/storage-quota-gib: "${STORAGE_SIZE_GB}"
  matchExpressions:
  - key: api.openshift.com/ccs
    operator: NotIn
    values:
    - "true"
  - key: hive.openshift.io/cluster-platform
    operator: In
    values: ["${CLOUD_PROVIDER}"]
